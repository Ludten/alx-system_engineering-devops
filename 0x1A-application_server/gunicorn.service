[Unit]
Description=Gunicorn web server
After=network.target

[Service]
Type=notify
WorkingDirectory = /home/ubuntu/AirBnB_clone_v4
ExecStart = /usr/bin/env gunicorn --workers 3 --access-logfile /tmp/airbnb-access.log --error-logfile /tmp/airbnb-error.log --bind 0.0.0.0:5003 web_dynamic.2-hbnb:app
ExecReload = /bin/kill -s HUP $MAINPID
KillMode=mixed
PrivateTmp=true

[Install]
WantedBy=multi-user.target
